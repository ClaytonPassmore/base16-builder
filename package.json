{
  "name": "base16-builder",
  "version": "0.0.18",
  "description": "",
  "repository": "alexbooker/base16-builder",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=5"
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist && cp -r db/ dist/",
    "test": "ava tests --require babel-register",
    "testw": "nodemon --watch lib --watch tests --exec npm run test",
    "pretest": "npm run build",
    "build": "node util/schemesPreviewGenerator.js && babel lib --out-dir dist && chmod +x dist/cli.js",
    "prepublish": "npm run build",
    "lint": "eslint ./lib",
    "commit": "git-cz"
  },
  "keywords": [
    "base16"
  ],
  "author": {
    "name": "Alex Booker",
    "email": "alexbooker@fastmail.im",
    "url": "booker.codes"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-3": "^6.3.13",
    "bluebird": "^3.3.3",
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.2.0",
    "eslint-config-google": "^0.4.0",
    "execa": "^0.2.2",
    "ghooks": "^1.0.3",
    "line-count": "^0.1.0",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "chalk": "^1.1.1",
    "fs-promise": "^0.3.1",
    "js-yaml": "^3.5.2",
    "meow": "^3.7.0",
    "nunjucks": "^2.3.0",
    "open": "0.0.5",
    "slug": "^0.9.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
